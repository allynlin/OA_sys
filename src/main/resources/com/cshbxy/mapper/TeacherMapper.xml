<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cshbxy.mapper.TeacherMapper">
    <select id="findDepartmentUid" resultType="String" parameterType="String">
        select departmentUid
        from teacher
        where uid = #{uid}
    </select>
    <select id="findRealeName" resultType="String" parameterType="String">
        select realeName
        from teacher
        where uid = #{uid}
    </select>
</mapper>