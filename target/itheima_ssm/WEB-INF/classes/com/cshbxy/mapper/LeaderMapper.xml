<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cshbxy.mapper.LeaderMapper">
    <select id="findRealeName" resultType="String" parameterType="String">
        select realeName
        from oa_sys.leader
        where uid = #{uid}
    </select>
</mapper>